<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryColor">#f0f8ff</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryColor">#c0c0c0</SolidColorBrush>

    <SolidColorBrush x:Key="TextColor">#7d7f7d</SolidColorBrush>
    <SolidColorBrush x:Key="AccentTextColor">#404040</SolidColorBrush>
    
    <SolidColorBrush x:Key="ControlTextColor">#d0d0d0</SolidColorBrush>
    <SolidColorBrush x:Key="ControlAccentTextColor">#f5f5f5</SolidColorBrush>
    
    <SolidColorBrush x:Key="ControlColor">#0047ab</SolidColorBrush>
    <SolidColorBrush x:Key="AccentColor">#1faee9</SolidColorBrush>
    
</ResourceDictionary>